{
  "name": "react-redux-material-boilerplate",
  "version": "0.0.0",
  "main": "server/app.js",
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.2",
    "history": "^4.9.0",
    "lodash": "~2.4.1",
    "moment": "^2.20.1",
    "primeicons": "^1.0.0",
    "primereact": "^3.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-addons-css-transition-group": "^15.0.2",
    "react-bootstrap": "^0.31.5",
    "react-data-grid": "^3.0.3",
    "react-data-grid-addons": "^3.0.3",
    "react-datepicker": "^1.0.4",
    "react-dom": "^16.6.3",
    "react-icons": "^3.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.4",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^4.0.0",
    "redbox-react": "^1.2.3",
    "redux": "^4.0.1",
    "redux-form": "^7.2.0",
    "redux-loading-promise-middleware": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "socket.io": "^1.0.6",
    "socket.io-client": "^1.0.6",
    "socketio-jwt": "^3.0.0",
    "universal-cookie": "^3.1.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-webpack-plugin": "^0.1.17",
    "connect-livereload": "~0.4.0",
    "cors": "^2.8.5",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "directory-named-webpack-plugin": "^2.3.0",
    "dotenv": "^8.0.0",
    "ejs-loader": "^0.3.0",
    "express": "^4.17.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "jit-grunt": "^0.5.0",
    "jshint-stylish": "~0.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "morgan": "^1.9.1",
    "node-sass": "^4.7.2",
    "open": "~0.0.4",
    "react-cosmos": "^4.0.0",
    "requirejs": "~2.1.11",
    "sass-loader": "^6.0.6",
    "should": "~3.3.1",
    "style-loader": "^0.19.0",
    "supertest": "~0.11.0",
    "time-grunt": "~0.3.1",
    "uglifyjs": "^2.4.11",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^2.11.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "cross-env node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack -p --progress --config webpack.config.build.js",
    "analyze": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --profile --json > stats.json && ./node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer stats.json",
    "cosmos": "node ./node_modules/react-cosmos/bin/cosmos",
    "cosmos:export": "node ./node_modules/react-cosmos/bin/cosmos-export"
  },
  "private": true,
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/no-webpack-loader-syntax": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-underscore-dangle": [
        2,
        {
          "allowAfterThis": true
        }
      ],
      "prefer-template": 2,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    }
  }
}
